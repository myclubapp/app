                }
              ],
              "styles": [
                "src/custom-themes/app-kadetten-unihockey/variables.scss",
                "src/custom-themes/app-kadetten-unihockey/global.scss"
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "app-uhc-win-u": {
              "index": "src/custom-themes/app-uhc-win-u/index.html",
              "assets": [
                {
                  "glob": "**/*.svg",
                  "input": "node_modules/ionicons/dist/ionicons/svg",
                  "output": "./svg"
                },
                {
                  "glob": "**/*",
                  "input": "src/custom-themes/app-uhc-win-u/assets",
                  "output": "./assets"
                },
                {
                  "glob": "manifest.webmanifest",
                  "input": "src/custom-themes/app-uhc-win-u",
                  "output": "./"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "styles": [
                "src/custom-themes/app-uhc-win-u/variables.scss",
                "src/custom-themes/app-uhc-win-u/global.scss"
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "app-stv-spreitenbach": {
              "index": "src/custom-themes/app-stv-spreitenbach/index.html",
              "assets": [
                {
                  "glob": "**/*.svg",
                  "input": "node_modules/ionicons/dist/ionicons/svg",
                  "output": "./svg"
                },
                {
                  "glob": "**/*",
                  "input": "src/custom-themes/app-stv-spreitenbach/assets",
                  "output": "./assets"
                },
                {
                  "glob": "manifest.webmanifest",
                  "input": "src/custom-themes/app-stv-spreitenbach",
                  "output": "./"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
