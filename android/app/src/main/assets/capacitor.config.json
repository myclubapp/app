{
	"appId": "app.myclub.defaultapp",
	"appName": "myclub",
	"webDir": "www/browser",
	"server": {
		"allowNavigation": [
			"http://developers.google.com/*"
		]
	},
	"android": {},
	"plugins": {
		"Badge": {
			"persist": true,
			"autoClear": false
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	},
	"ios": {
		"scheme": "App"
	}
}
